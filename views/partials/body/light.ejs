<div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            전등 사용 정보
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                                <thead>
                                    <tr>
                                        <th>날짜</th>
                                        <th>시간</th>
                                        <th>상태</th>
                                        <th>사용자</th>

                                    </tr>
                                </thead>
                                <tbody>
                                  <% data.forEach(function(e){%>
                                    <tr class="odd gradeX">
                                        <td><%=e.date%></td>
                                        <td><%=e.time%></td>
                                        <td><%=e.status%></td>
                                        <td class="center">관리자</td>
                                    </tr>
                                  <%})%>

                                </tbody>
                            </table>
                            <!-- /.table-responsive -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
